{
    "name": "uboratechno/shinobi",
    "description": "Roles and Permissions for laravel 6 - 10",
    "keywords": [
        "auth",
        "laravel",
        "permission",
        "permissions",
        "roles",
        "acl",
        "rbac",
        "security"
    ],
    "license": "MIT",
    "authors": [{
        "name": "Playbert Ronaldo",
        "email": "playbert.ronaldo@gmail.com"
    }],
    "require": {
        "php" : "^7.3|^8.0|^8.1",
        "illuminate/support": "^6.0|^7.0|^8.0|^9.0|^10.0"
    },
    "autoload": {
        "psr-4": {
            "Uboratechno\\Shinobi\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Uboratechno\\Shinobi\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Uboratechno\\Shinobi\\ShinobiServiceProvider"
            ],
            "aliases": {
                "Shinobi": "Uboratechno\\Shinobi\\Facades\\Shinobi"
            }
        }
    },
    "minimum-stability": "dev",
    "require-dev": {
        "orchestra/testbench": "^6.0|^7.0|^8.0",
        "doctrine/dbal": "3.6.2"
    }
}
